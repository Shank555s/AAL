#include<stdio.h>
#include<string.h>

int main() {
    char A[] = "nobody noticed him";  
    char B[] = "him";                 
    int len = strlen(A);         
    int length = strlen(B);         
    int i, j, count = 0;            

   
    for (i = 0; i <= len - length; i++) {
        
        for (j = 0; j < length; j++) {
            if (A[i + j] != B[j]) {
                break;  
            }
        }

        
        if (j == length) {
           
            printf("Found at index %d\n", i);
        }
    }

    
    
}
